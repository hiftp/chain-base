<?xml version="1.0" encoding="utf-8"?>
<odoo>
   <data noupdate="1">
    <function model="ir.model.data" name="write">
        <function name="search" model="ir.model.data">
            <value eval="[('module', '=', 'base'), ('name', '=', 'main_partner' )]"/>
        </function>
        <value eval="{'noupdate': False}"/>
    </function>

    <record id="base.main_partner" model="res.partner">
        <field name="name">Saltfun Store</field>
        <field name="image_1920" type="base64" file="pl_curved_backend_theme/static/description/img/logo/logo_login.png"/>
    </record>

    <function model="ir.model.data" name="write">
        <function name="search" model="ir.model.data">
            <value eval="[('module', '=', 'base'), ('name', '=', 'main_partner' )]"/>
        </function>
        <value eval="{'noupdate': True}"/>
    </function>
    </data>
</odoo>