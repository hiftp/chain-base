<odoo>

    <record id="email_template_project_scrum" model="mail.template">
        <field name="name">Scrum Meeting</field>
        <field name="email_from"/>
        <field name="subject">${object.name or ''}</field>
        <field name="email_to"/>
        <field name="model_id"
               ref="project_scrum_agile.model_project_scrum_meeting"/>
        <field name="auto_delete" eval="False"/>
        <field name="body_html">
        </field>
    </record>

</odoo>
