<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_blog_post_form_editor" model="ir.ui.view">
		<field name="inherit_id" ref="website_blog.view_blog_post_form" />
		<field name="name">blog.post.form</field>
		<field name="model">blog.post</field>
		<field name="priority">16</field>
		<field name="arch" type="xml">
			<xpath expr="//page[@name='seo']" position="before">
				<page name="content" string="Content" groups="base.group_no_one">
					<field name="content" widget="html" modifiers="{}" />
				</page>
			</xpath>
		</field>
	</record>	
</odoo>


